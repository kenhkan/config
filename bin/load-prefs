#!/usr/bin/env bash

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
HERE="$( cd "$DIR/.." && pwd)"
PREF_DIR=$HOME/Library
OUT_NAME=$HERE/prefs

cd $HERE

>&2 echo "----> Copying \"$OUT_NAME/*\" to \"$PREF_DIR/\"..."
cp -R $OUT_NAME/* $PREF_DIR/

>&2 echo "----> Done loading preferences."
exit 0
